{
  "name": "@yishanzhilu/tai",
  "version": "0.0.1",
  "author": "Yishan Authors",
  "license": "MIT",
  "private": true,
  "scripts": {
    "install:lock": "yarn install --frozen-lockfile",
    "dev": "next -p 10000",
    "build": "next build",
    "build:analyzer": "ANALYZE=true next build",
    "start": "next start -p 10000",
    "test": "jest --onlyChanged",
    "test:updateSnapshot": "jest --onlyChanged --updateSnapshot",
    "test:full": "jest --coverage",
    "verify": "npm-run-all compile -p test:full lint",
    "lint": "eslint ./ --ext .js,.ts,.tsx",
    "lint:fix": "eslint ./ --ext .js,.ts,.tsx --fix",
    "git-cz": "git-cz -s"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "dependencies": {
    "@next/bundle-analyzer": "^9.1.5",
    "@yishanzhilubp/core": "^4.0.0",
    "@yishanzhilubp/datetime": "^4.0.0",
    "@yishanzhilubp/select": "^4.0.0",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "js-cookie": "^2.2.1",
    "next": "^9.1.3",
    "next-cookies": "^2.0.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "nprogress": "^0.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^3.29.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.4",
    "@types/jest": "^24.0.23",
    "@types/js-cookie": "^2.2.4",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-test-renderer": "^16.9.1",
    "@types/react-virtualized": "^9.21.6",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "typescript": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:yishanzhilu/tai.git"
  }
}
